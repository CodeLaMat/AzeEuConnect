{
  "name": "AzeEU",
  "private": true,
  "packageManager": "npm@10.8.3",
  "engines": {
    "node": ">=18"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "check-types": "turbo run check-types",
    "start": "concurrently \"npm run start --workspace=frontend\" \"npm run start --workspace=backend\""
  },
  "devDependencies": {
    "prettier": "^3.5.3",
    "turbo": "^2.4.4",
    "typescript": "^5.8.2",
    "concurrently": "^8.0.1"
  }
}
